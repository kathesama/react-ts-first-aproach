{
  "name": "react-ts-first-aproach",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --detectOpenHandles=true",
    "test:verbose": "react-scripts test --detectOpenHandles=true --verbose",
    "test:coverage": "react-scripts test --coverage",
    "eject": "react-scripts eject",
    "lint": "eslint ./src/**/*.{ts,tsx} --ext .ts,.tsx",
    "lint:fix": "eslint ./src/**/*.{ts,tsx} --ext .ts,.tsx --fix",
    "prettier:fix": "prettier --write",
    "prettier:check": "prettier --check ."
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.1",
    "axios": "^0.26.1",
    "i18next": "^21.6.16",
    "npm-check-updates": "^16.7.4",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-hook-form": "^7.29.0",
    "react-i18next": "^11.16.7",
    "react-redux": "^7.2.8",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "redux-persist": "^6.0.0",
    "sass": "^1.50.0",
    "web-vitals": "^2.1.4",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.0.1",
    "@testing-library/user-event": "^13.5.0",
    "@types/enzyme": "^3.10.12",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.23",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/react-redux": "^7.1.24",
    "@types/react-router-dom": "^5.3.3",
    "@types/redux": "^3.6.0",
    "@types/redux-thunk": "^2.1.0",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "axios-mock-adapter": "^1.20.0",
    "enzyme": "^3.11.0",
    "eslint": "8.22.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "eslint-plugin-security": "^1.4.0",
    "git-commit-msg-linter": "^4.1.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "msw": "^0.39.2",
    "prettier": "^2.6.2",
    "typescript": "^4.6.3"
  },
  "lint-staged": {
    "src/*.{js,jsx,ts,tsx}": [
      "npm run prettier:fix",
      "npm run lint"
    ]
  },
  "resolutions": {
    "@types/react": "18.0.0",
    "@types/react-dom": "18.0.0"
  },
  "engines": {
    "yarn": "YARN NO LONGER USED - use npm instead."
  }
}
